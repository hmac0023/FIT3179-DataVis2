{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "title": "Number of overweight and obese individuals over time in Australia",
    "description": "Line chart of individuals considered overweight and obese in Australia over time",
    "width": 1000,
    "height": 500,
    "data": {
      "url": "https://raw.githubusercontent.com/hmac0023/FIT3179-DataVis2/refs/heads/main/OverweightAndObesePeopleOverTime.csv",
      "format": {"type": "csv"}
    },
    "mark": "line",
    "encoding": {
      "x": {
        "field": "Year",
        "type": "temporal",
        "axis": {"title": "Year"}
      },
      "y": {
        "field": "Estimates",
        "type": "quantitative",
        "axis": {"title": "Estimate (Number of People)"}
      },
      "tooltip": [
        {"field": "Year", "type": "temporal", "title": "Year"},
        {"field": "Estimates", "type": "quantitative", "title": "Estimate"}
      ]
    }
  }
  